#include <stdio.h>

int main() {
//printf("Harsha")
    int n, sum = 0;
    
    printf("Enter the number of values to be summed: ");
    scanf("%d", &n);
    
    for (int i = 0; i < n; i++) {
        int value;
        printf("Enter value #%d: ", i + 1);
        scanf("%d", &value);
        sum += value;
    }
    
    printf("The sum of the %d values is: %d", n, sum);
    
    return 0;
}
